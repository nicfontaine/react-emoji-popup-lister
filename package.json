{
  "name": "react-emoji-popup-lister",
  "description": "React emoji search & select by colon syntax, with keyboard navigation",
  "version": "0.2.0",
  "author": "Nic Fontaine <nic@nicfontaine.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nicfontaine/react-emoji-popup-lister"
  },
  "private": false,
  "main": "dist/index.js",
	"types": "dist/index.d.ts",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "fuzzy-search": "^3.2.1",
    "gemoji": "^8.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "mkdir -p dist && NODE_ENV=production babel --extensions .ts ./src/lib -d ./dist",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
		"prepublish": "tsc"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "react",
    "emoji",
    "input",
    "textarea",
    "component",
    "picker",
    "keyboard",
    "search"
